# AIShell Environment Configuration
# Copy this file to .env and fill in your actual values

# LLM Provider API Keys
ANTHROPIC_API_KEY=your-claude-api-key-here
OPENAI_API_KEY=your-openai-api-key-here
GOOGLE_API_KEY=your-gemini-api-key-here

# LLM Provider URLs
OLLAMA_URL=http://localhost:11434
OPENAI_BASE_URL=https://api.openai.com/v1

# MCP Server URLs (add your MCP servers here)
MCP_SERVER_1=http://localhost:8000
MCP_SERVER_2=http://localhost:8001

# Default LLM Settings
DEFAULT_LLM_PROVIDER=claude
DEFAULT_LLM_MODEL=claude-3-sonnet-20240229
DEFAULT_TEMPERATURE=0.7
DEFAULT_MAX_TOKENS=4096

# Shell Configuration
SHELL_HISTORY_SIZE=1000
SHELL_NL_PROVIDER=claude

# File Search Settings
SPOTLIGHT_ENABLED=true
MAX_SEARCH_RESULTS=100

# Logging
LOG_LEVEL=INFO
TRANSCRIPT_ENABLED=true

# Other Configuration
PLAYWRIGHT_HEADLESS=true
WEB_SEARCH_ENGINE=google
WEB_SEARCH_RESULTS=10